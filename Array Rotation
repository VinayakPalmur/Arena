public static void rotate(int arr[], int n, int d){
//enter your code here
for (int i = 0; i < d; i++) 
{
    leftRotatebyOne(arr, n);
}
}
public static void leftRotatebyOne(int arr[], int n) 
{ 
int i, temp; 
temp = arr[0]; 
for (i = 0; i < n - 1; i++) 
arr[i] = arr[i + 1]; // Left shift by one
arr[i] = temp; 
}
